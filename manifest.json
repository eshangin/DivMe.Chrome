{
    "manifest_version": 2,
    "name": "Overlay div",
    "version": "0.1",
    "permissions": ["storage", "tabs"],
    "background": {
        "page": "background/index.html",
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup/index.html"
    },
    "content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content.js"
			]
		}
	]
}